/**index.wxss**/
.list{
  display: flex;
  justify-content: space-between;
  padding: 10rpx 20rpx;
  border: 1rpx solid #ddd;

}
.list .text{
  width: 430rpx;
}
.list .img{
  width: 250rpx;
  height: 160rpx;
}
.list image{
  width: 250rpx;
  height: 160rpx;
}
.list text{
  display: block;
  text-align: left;
  margin: 20rpx 0rpx;
}
.list .address{
  font-size: 24rpx;
}